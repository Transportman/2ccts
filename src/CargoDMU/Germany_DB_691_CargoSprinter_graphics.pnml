//Dualheaded engines
switch(FEAT_TRAINS, SELF, switch_cargodmu_Germany_DB_691_CargoSprinter_reversed, var[0xC8]) {
	0xFE: switch_cargodmu_Germany_DB_691_CargoSprinter_back_livery;
    0xFF: switch_cargodmu_Germany_DB_691_CargoSprinter_back_livery; 
    switch_cargodmu_Germany_DB_691_CargoSprinter_front_livery;
}

//Visual effect, for DMU this is done on the front engine
switch(FEAT_TRAINS, SELF, switch_cargodmu_Germany_DB_691_CargoSprinter_visual_effect_and_powered, var[0xC8]) {
	0xFE: visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
	0xFF: visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
	visual_effect_and_powered(VISUAL_EFFECT_DIESEL, -3, DISABLE_WAGON_POWER);
}

