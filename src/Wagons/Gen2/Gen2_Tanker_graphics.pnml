	OIL_: switch_wagon_Gen2_Tanker_Oil_livery;
	GOOD: switch_wagon_Gen2_Tanker_Goods_livery;
	RUBR: switch_wagon_Gen2_Tanker_Rubber_livery;
	PLST: switch_wagon_Gen2_Tanker_Plastic_livery;
	PETR: switch_wagon_Gen2_Tanker_Oil_livery;
	PLAS: switch_wagon_Gen2_Tanker_Plastic_livery;
	RFPR: switch_wagon_Gen2_Tanker_Oil_livery;
	switch_wagon_Gen2_Tanker_Water_livery; //WATR, COLA, BEER, DYES, MILK
}

